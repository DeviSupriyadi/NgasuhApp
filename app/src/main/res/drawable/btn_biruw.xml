<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="@color/putih"/>
            <corners android:radius="@dimen/dm14"/>
        </shape>
    </item>
    <item android:right="@dimen/dm1"
        android:left="@dimen/dm1"
        android:top="@dimen/dm1"
        android:bottom="@dimen/dm1">
        <selector>
            <item android:state_pressed="true">
                <shape>
                    <solid android:color="@color/birutua"/>
                    <corners android:radius="@dimen/dm14"/>
                </shape>
            </item>
            <item>
                <shape>
                    <solid android:color="@color/biru"/>
                    <corners android:radius="@dimen/dm14"/>
                </shape>
            </item>
        </selector>
    </item>

</layer-list>